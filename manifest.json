{
    "name": "Kapi - Copy that!",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_icon": "kapi-logo.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "kapi-logo.png"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com object-src 'self'"
}